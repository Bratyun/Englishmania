@model IEnumerable<Englishmania.DAL.Entities.WordVocabulary>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.VocabularyId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WordId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.VocabularyId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.WordId)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.WordId">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.WordId">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.WordId">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
